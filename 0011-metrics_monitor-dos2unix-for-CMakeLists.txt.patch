---
 samples/metrics_monitor/CMakeLists.txt | 138 ++++++++++++-------------
 1 file changed, 69 insertions(+), 69 deletions(-)

diff --git a/samples/metrics_monitor/CMakeLists.txt b/samples/metrics_monitor/CMakeLists.txt
index 5f73970..30b2d52 100755
--- a/samples/metrics_monitor/CMakeLists.txt
+++ b/samples/metrics_monitor/CMakeLists.txt
@@ -1,70 +1,70 @@
-# metrics_monitor library
-
-include_directories (
-  ${CMAKE_CURRENT_SOURCE_DIR}/include/
-)
-
-set( sources "" )
-
-file( GLOB_RECURSE srcs "${CMAKE_CURRENT_SOURCE_DIR}/src/*.cpp" )
-list( APPEND sources ${srcs})
-
-make_library( cttmetrics none shared "nosafestring" )
-make_library( cttmetrics_static none static "nosafestring" )
-
-# metrics_monitor sample
-
-set( sources "" )
-
-file( GLOB_RECURSE srcs "${CMAKE_CURRENT_SOURCE_DIR}/sample/*.cpp" )
-list( APPEND sources ${srcs})
-list( APPEND LIBS cttmetrics)
-
-make_executable( metrics_monitor none "nosafestring" )
-
-# test_monitor
-
-pkg_check_modules(PKG_LIBDRM    libdrm)
-pkg_check_modules(PKG_PCIACCESS pciaccess)
-
-if( PKG_LIBDRM_FOUND AND
-    PKG_PCIACCESS_FOUND AND
-    DEFINED GTEST_FOUND)
-
-    set( sources "" )
-
-    list ( APPEND incldirs
-        ${GTEST_INCLUDE}/gtest
-        ${GTEST_INCLUDE}/gtest/internal
-        ${CMAKE_CURRENT_SOURCE_DIR}/test )
-
-    foreach( dir ${incldirs} )
-      include_directories( ${dir} )
-    endforeach( )
-
-    file( GLOB_RECURSE srcs "${CMAKE_CURRENT_SOURCE_DIR}/test/*.cpp" )
-    list( APPEND sources ${srcs})
-    list( APPEND LIBS
-      pthread
-      rt
-      gtest )
-
-    set(DEPENDENCIES libdrm)
-
-    make_executable( test_monitor none "nosafestring" )
-
-else()
-
-    if( NOT PKG_PCIACCESS_FOUND )
-        message( STATUS "pciaccess was not found (optional), the following will not be built: test_monitor." )
-    endif()
-
-    if( NOT PKG_LIBDRM_FOUND )
-        message( STATUS "libdrm was not found (optional), the following will not be built: test_monitor." )
-    endif()
-
-    if( NOT DEFINED GTEST_FOUND )
-        message( STATUS "Google tests was not found (optional), the following will not be built: test_monitor." )
-    endif()
-
+# metrics_monitor library
+
+include_directories (
+  ${CMAKE_CURRENT_SOURCE_DIR}/include/
+)
+
+set( sources "" )
+
+file( GLOB_RECURSE srcs "${CMAKE_CURRENT_SOURCE_DIR}/src/*.cpp" )
+list( APPEND sources ${srcs})
+
+make_library( cttmetrics none shared "nosafestring" )
+make_library( cttmetrics_static none static "nosafestring" )
+
+# metrics_monitor sample
+
+set( sources "" )
+
+file( GLOB_RECURSE srcs "${CMAKE_CURRENT_SOURCE_DIR}/sample/*.cpp" )
+list( APPEND sources ${srcs})
+list( APPEND LIBS cttmetrics)
+
+make_executable( metrics_monitor none "nosafestring" )
+
+# test_monitor
+
+pkg_check_modules(PKG_LIBDRM    libdrm)
+pkg_check_modules(PKG_PCIACCESS pciaccess)
+
+if( PKG_LIBDRM_FOUND AND
+    PKG_PCIACCESS_FOUND AND
+    DEFINED GTEST_FOUND)
+
+    set( sources "" )
+
+    list ( APPEND incldirs
+        ${GTEST_INCLUDE}/gtest
+        ${GTEST_INCLUDE}/gtest/internal
+        ${CMAKE_CURRENT_SOURCE_DIR}/test )
+
+    foreach( dir ${incldirs} )
+      include_directories( ${dir} )
+    endforeach( )
+
+    file( GLOB_RECURSE srcs "${CMAKE_CURRENT_SOURCE_DIR}/test/*.cpp" )
+    list( APPEND sources ${srcs})
+    list( APPEND LIBS
+      pthread
+      rt
+      gtest )
+
+    set(DEPENDENCIES libdrm)
+
+    make_executable( test_monitor none "nosafestring" )
+
+else()
+
+    if( NOT PKG_PCIACCESS_FOUND )
+        message( STATUS "pciaccess was not found (optional), the following will not be built: test_monitor." )
+    endif()
+
+    if( NOT PKG_LIBDRM_FOUND )
+        message( STATUS "libdrm was not found (optional), the following will not be built: test_monitor." )
+    endif()
+
+    if( NOT DEFINED GTEST_FOUND )
+        message( STATUS "Google tests was not found (optional), the following will not be built: test_monitor." )
+    endif()
+
 endif()
\ No newline at end of file
